{
    "root": "build/",
    "https_only": true,
    "headers": {
        "/**": {
            "Strict-Transport-Security": "max-age=31536000",
            "Content-Security-Policy": "default-src 'none'; connect-src https://ensemble-transposer.herokuapp.com/; script-src 'self' 'unsafe-inline' https://www.google-analytics.com/; font-src 'self'; img-src 'self' data:; style-src 'self' 'unsafe-inline' blob:; form-action 'none'; base-uri 'none'; frame-ancestors 'none';",
            "X-Content-Type-Options": "nosniff",
            "X-Frame-Options": "deny",
            "X-XSS-Protection": "1; mode=block"
        },
        "/__heartbeat__": {
            "Cache-Control": "no-cache, no-store, must-revalidate",
            "Pragma": "no-cache",
            "Expires": 0
        },
        "/__lbheartbeat__": {
            "Cache-Control": "no-cache, no-store, must-revalidate",
            "Pragma": "no-cache",
            "Expires": 0
        }
    },
    "routes": {
        "/": "index.html",
        "/dashboard/user-activity": "index.html",
        "/dashboard/usage-behavior": "index.html",
        "/dashboard/hardware": "index.html",
        "/__heartbeat__": "/ops/heartbeat.html",
        "/__lbheartbeat__": "/ops/lbheartbeat.html",
        "/__version__": "/ops/version.json"
    }
}
