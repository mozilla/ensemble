{
    "name": "ensemble",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "start": "npm-run-all --parallel watch:css start:app",
        "watch:css": "stylus --watch --sourcemap src/components/views/styl/* --out ./src/components/views/css/",
        "start:app": "react-scripts start",
        "build": "npm-run-all build:css build:app",
        "build:app": "react-scripts build",
        "build:css": "stylus --sourcemap ./src/components/views/styl/* --out ./src/components/views/css/",
        "build:version.json": "node scripts/generateVersionJSON",
        "eject": "react-scripts eject",
        "test": "npm-run-all lint test:jest test:nightwatch:dev",
        "lint": "npm-run-all lint:*",
        "lint:js-extra": "npx eslint --config .eslintrc.extra.js --ext .js --ext .json .",
        "lint:styl": "stylint src/components/views/styl",
        "test:jest": "CI=true react-scripts test",
        "test:nightwatch:dev": "node scripts/runNightwatch dev",
        "test:nightwatch:stage": "node scripts/runNightwatch stage",
        "test:nightwatch:prod": "node scripts/runNightwatch prod",
        "test:nightwatch:browserstack:dev": "BS=true node scripts/runNightwatch dev",
        "test:nightwatch:browserstack:stage": "BS=true node scripts/runNightwatch stage",
        "test:nightwatch:browserstack:prod": "BS=true node scripts/runNightwatch prod",
        "posttest": "npm audit || true",
        "size": "source-map-explorer build/static/js/main.*"
    },
    "dependencies": {
        "babel-polyfill": "6.26.0",
        "d3-scale": "3.0.0",
        "d3-selection": "1.4.0",
        "d3-shape": "1.3.5",
        "d3-transition": "1.2.0",
        "dateformat": "3.0.3",
        "distinct-colors": "1.0.4",
        "markdown-it": "8.4.2",
        "markdown-it-sup": "1.0.0",
        "memoize-one": "5.0.4",
        "metrics-graphics": "2.15.6",
        "npm-run-all": "4.1.5",
        "react": "16.8.6",
        "react-app-polyfill": "1.0.0",
        "react-dom": "16.8.6",
        "react-ga": "2.5.7",
        "react-helmet": "5.2.0",
        "react-loadable": "5.5.0",
        "react-metrics-graphics": "1.0.6",
        "react-refetch": "2.0.3",
        "react-router-dom": "5.0.0",
        "react-scripts": "3.0.0",
        "react-spinners": "0.5.4",
        "stylus": "0.54.5"
    },
    "devDependencies": {
        "browserstack-automate": "1.0.2",
        "chromedriver": "74.0.0",
        "enzyme": "3.9.0",
        "enzyme-adapter-react-16": "1.12.1",
        "eslint-plugin-jest": "22.5.1",
        "eslint-plugin-json": "1.4.0",
        "eslint-plugin-jsx-a11y": "6.2.1",
        "eslint-plugin-react": "7.12.4",
        "geckodriver": "1.12.2",
        "nightwatch": "1.0.19",
        "react-test-renderer": "16.8.6",
        "request": "2.88.0",
        "source-map-explorer": "1.8.0",
        "stylint": "1.5.9"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 10",
        "not op_mini all"
    ]
}
