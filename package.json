{
    "name": "ensemble",
    "version": "0.2.0",
    "private": true,
    "homepage": "https://metrics.mozilla.com/protected/usage-report-demo",
    "scripts": {
        "start": "react-scripts start",
        "build": "npm-run-all build:*",
        "build:app": "react-scripts build",
        "build:version.json": "node scripts/generateVersionJSON",
        "eject": "react-scripts eject",
        "validate": "npm run test",
        "test": "npm-run-all lint test:*",
        "test:jest": "CI=true react-scripts test --env=jsdom",
        "test:nightwatch": "npm-run-all test:nightwatch:*",
        "test:nightwatch:js-disabled": "nightwatch --env jsDisabled --tag 'js-disabled'",
        "test:nightwatch:js-enabled": "nightwatch --env default,chrome,safari --skiptags 'js-disabled'",
        "lint": "npm-run-all lint:*",
        "lint:js-extra": "eslint --config .eslintrc.extra.js --ext .js --ext .json .",
        "lint:css": "stylelint src/**/*.css",
        "posttest": "npm audit || true",
        "size": "source-map-explorer build/static/js/main.*"
    },
    "dependencies": {
        "babel-polyfill": "6.26.0",
        "d3-scale": "2.1.0",
        "d3-selection": "1.3.0",
        "d3-shape": "1.2.0",
        "dateformat": "3.0.3",
        "distinct-colors": "1.0.4",
        "markdown-it": "8.4.1",
        "metrics-graphics": "2.15.6",
        "react": "16.4.1",
        "react-dom": "16.4.1",
        "react-ga": "2.5.3",
        "react-helmet": "5.2.0",
        "react-loadable": "5.4.0",
        "react-metrics-graphics": "1.0.6",
        "react-refetch": "1.0.4",
        "react-router-dom": "4.3.1",
        "react-scripts": "1.1.4",
        "react-spinners": "0.3.2",
        "whatwg-fetch": "2.0.4"
    },
    "devDependencies": {
        "babel-eslint": "8.2.5",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-16": "1.1.1",
        "eslint": "5.0.0",
        "eslint-plugin-jest": "21.17.0",
        "eslint-plugin-json": "1.2.0",
        "eslint-plugin-jsx-a11y": "6.0.3",
        "eslint-plugin-react": "7.10.0",
        "geckodriver": "1.11.0",
        "jest-cli": "23.2.0",
        "nightwatch": "0.9.21",
        "npm-run-all": "4.1.3",
        "react-test-renderer": "16.4.1",
        "request": "2.87.0",
        "selenium-server-standalone-jar": "3.12.0",
        "source-map-explorer": "1.5.0",
        "stylelint": "9.3.0",
        "stylelint-config-standard": "18.2.0"
    }
}
