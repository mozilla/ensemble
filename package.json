{
    "name": "ensemble",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "validate": "npm run test",
        "test": "npm-run-all lint test:jest test:nightwatch:local",
        "test:jest": "CI=true react-scripts test --env=jsdom",
        "test:nightwatch:local": "npm-run-all test:nightwatch:local:*",
        "test:nightwatch:local:js-disabled": "nightwatch --config nightwatch.local.conf.js --env jsDisabled --tag 'js-disabled'",
        "test:nightwatch:local:js-enabled": "nightwatch --config nightwatch.local.conf.js --env default,chrome,safari --skiptags 'js-disabled'",
        "test:nightwatch:stage": "npm-run-all test:nightwatch:stage:*",
        "test:nightwatch:stage:js-disabled": "nightwatch --config nightwatch.stage.conf.js --env jsDisabled --tag 'js-disabled'",
        "test:nightwatch:stage:js-enabled": "nightwatch --config nightwatch.stage.conf.js --env default,chrome,safari --skiptags 'js-disabled'",
        "test:nightwatch:prod": "npm-run-all test:nightwatch:prod:*",
        "test:nightwatch:prod:js-disabled": "nightwatch --config nightwatch.prod.conf.js --env jsDisabled --tag 'js-disabled'",
        "test:nightwatch:prod:js-enabled": "nightwatch --config nightwatch.prod.conf.js --env default,chrome,safari --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:local": "npm-run-all --sequential 'test:nightwatch:browserstack:env:* -- --config nightwatch.browserstack.local.conf.js'",
        "test:nightwatch:browserstack:stage": "npm-run-all --sequential 'test:nightwatch:browserstack:env:* -- --config nightwatch.browserstack.stage.conf.js'",
        "test:nightwatch:browserstack:prod": "npm-run-all --sequential 'test:nightwatch:browserstack:env:* -- --config nightwatch.browserstack.prod.conf.js'",
        "test:nightwatch:browserstack:env:js-disabled": "./src/tests/nightwatch/runners/browserStackRunner.js --env jsDisabled --tag 'js-disabled'",
        "test:nightwatch:browserstack:env:ie": "./src/tests/nightwatch/runners/browserStackRunner.js --env ie --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:env:edge": "./src/tests/nightwatch/runners/browserStackRunner.js --env edge --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:env:firefoxWin7": "./src/tests/nightwatch/runners/browserStackRunner.js --env firefoxWin7 --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:env:firefoxSierra": "./src/tests/nightwatch/runners/browserStackRunner.js --env firefoxSierra --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:env:chromeWin7": "./src/tests/nightwatch/runners/browserStackRunner.js --env chromeWin7 --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:env:chromeSierra": "./src/tests/nightwatch/runners/browserStackRunner.js --env chromeSierra --skiptags 'js-disabled'",
        "lint": "npm-run-all lint:*",
        "lint:js-extra": "eslint --config .eslintrc.extra.js --ext .js --ext .json .",
        "lint:css": "stylelint src/**/*.css",
        "posttest": "npm audit || true",
        "size": "source-map-explorer build/static/js/main.*"
    },
    "dependencies": {
        "babel-polyfill": "6.26.0",
        "d3-scale": "2.1.2",
        "d3-selection": "1.3.2",
        "d3-shape": "1.2.2",
        "d3-transition": "1.1.3",
        "dateformat": "3.0.3",
        "distinct-colors": "1.0.4",
        "markdown-it": "8.4.2",
        "markdown-it-sup": "1.0.0",
        "memoize-one": "4.0.0",
        "metrics-graphics": "2.15.6",
        "npm-run-all": "4.1.3",
        "react": "16.4.2",
        "react-dom": "16.4.2",
        "react-ga": "2.5.3",
        "react-helmet": "5.2.0",
        "react-loadable": "5.5.0",
        "react-metrics-graphics": "1.0.6",
        "react-refetch": "2.0.0",
        "react-router-dom": "4.3.1",
        "react-scripts": "1.1.5",
        "react-spinners": "0.4.5"
    },
    "devDependencies": {
        "babel-eslint": "8.2.6",
        "browserstack-local": "1.3.4",
        "enzyme": "3.5.0",
        "enzyme-adapter-react-16": "1.3.0",
        "eslint": "5.5.0",
        "eslint-plugin-jest": "21.22.0",
        "eslint-plugin-json": "1.2.1",
        "eslint-plugin-jsx-a11y": "6.1.1",
        "eslint-plugin-react": "7.11.1",
        "geckodriver": "1.12.2",
        "nightwatch": "0.9.21",
        "react-test-renderer": "16.4.2",
        "request": "2.88.0",
        "selenium-server-standalone-jar": "3.13.0",
        "source-map-explorer": "1.6.0",
        "stylelint": "9.5.0",
        "stylelint-config-standard": "18.2.0"
    }
}
