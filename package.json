{
    "name": "ensemble",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "npm-run-all build:*",
        "build:app": "react-scripts build",
        "build:version.json": "node scripts/generateVersionJSON",
        "eject": "react-scripts eject",
        "validate": "npm run test",
        "test": "npm-run-all lint test:*",
        "test:jest": "CI=true react-scripts test --env=jsdom",
        "test:nightwatch": "nightwatch --env default,chrome,safari",
        "lint": "npm-run-all lint:*",
        "lint:js-extra": "eslint --config .eslintrc.extra.js --ext .js --ext .json .",
        "lint:css": "stylelint src/**/*.css",
        "lint:deps": "nsp check --warn-only",
        "size": "source-map-explorer build/static/js/main.*"
    },
    "dependencies": {
        "babel-polyfill": "6.26.0",
        "d3-scale": "2.0.0",
        "d3-selection": "1.3.0",
        "d3-shape": "1.2.0",
        "dateformat": "3.0.3",
        "distinct-colors": "1.0.4",
        "markdown-it": "8.4.1",
        "metrics-graphics": "2.15.3",
        "react": "16.4.0",
        "react-dom": "16.4.0",
        "react-ga": "2.5.3",
        "react-helmet": "5.2.0",
        "react-loadable": "5.4.0",
        "react-metrics-graphics": "1.0.3",
        "react-refetch": "1.0.4",
        "react-router-dom": "4.2.2",
        "react-scripts": "1.1.4",
        "react-spinners": "0.3.2",
        "whatwg-fetch": "2.0.4"
    },
    "devDependencies": {
        "babel-eslint": "8.2.3",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-16": "1.1.1",
        "eslint": "4.19.1",
        "eslint-plugin-jest": "21.17.0",
        "eslint-plugin-json": "1.2.0",
        "eslint-plugin-jsx-a11y": "6.0.3",
        "eslint-plugin-react": "7.9.1",
        "geckodriver": "1.11.0",
        "nightwatch": "0.9.21",
        "npm-run-all": "4.1.3",
        "nsp": "3.2.1",
        "react-test-renderer": "16.4.0",
        "request": "2.87.0",
        "selenium-server-standalone-jar": "3.12.0",
        "source-map-explorer": "1.5.0",
        "stylelint": "9.2.1",
        "stylelint-config-standard": "18.2.0"
    }
}
