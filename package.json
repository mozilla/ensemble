{
    "name": "ensemble",
    "version": "0.2.0",
    "private": true,
    "homepage": "https://metrics.mozilla.com/protected/usage-report-demo",
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "start": "node production-server",
        "dev": "react-scripts start",
        "build": "npm-run-all build:*",
        "build:app": "react-scripts build",
        "build:version.json": "node scripts/generateVersionJSON",
        "eject": "react-scripts eject",
        "validate": "npm run test",
        "test": "npm-run-all lint test:jest test:nightwatch:local",
        "test:jest": "CI=true react-scripts test --env=jsdom",
        "test:nightwatch:local": "npm-run-all test:nightwatch:local:*",
        "test:nightwatch:local:js-disabled": "nightwatch --config nightwatch.local.conf.js --env jsDisabled --tag 'js-disabled'",
        "test:nightwatch:local:js-enabled": "nightwatch --config nightwatch.local.conf.js --env default,chrome,safari --skiptags 'js-disabled'",
        "test:nightwatch:browserstack": "npm-run-all --sequential test:nightwatch:browserstack:*",
        "test:nightwatch:browserstack:js-disabled": "./src/tests/nightwatch/runners/browserstack.js --config nightwatch.browserstack.conf.js --env jsDisabled --tag 'js-disabled'",
        "test:nightwatch:browserstack:ie": "./src/tests/nightwatch/runners/browserstack.js --config nightwatch.browserstack.conf.js --env ie --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:edge": "./src/tests/nightwatch/runners/browserstack.js --config nightwatch.browserstack.conf.js --env edge --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:safari": "./src/tests/nightwatch/runners/browserstack.js --config nightwatch.browserstack.conf.js --env safari --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:firefoxWin7": "./src/tests/nightwatch/runners/browserstack.js --config nightwatch.browserstack.conf.js --env firefoxWin7 --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:firefoxSierra": "./src/tests/nightwatch/runners/browserstack.js --config nightwatch.browserstack.conf.js --env firefoxSierra --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:chromeWin7": "./src/tests/nightwatch/runners/browserstack.js --config nightwatch.browserstack.conf.js --env chromeWin7 --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:chromeSierra": "./src/tests/nightwatch/runners/browserstack.js --config nightwatch.browserstack.conf.js --env chromeSierra --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:operaWin7": "./src/tests/nightwatch/runners/browserstack.js --config nightwatch.browserstack.conf.js --env operaWin7 --skiptags 'js-disabled'",
        "test:nightwatch:browserstack:operaSierra": "./src/tests/nightwatch/runners/browserstack.js --config nightwatch.browserstack.conf.js --env operaSierra --skiptags 'js-disabled'",
        "lint": "npm-run-all lint:*",
        "lint:js-extra": "eslint --config .eslintrc.extra.js --ext .js --ext .json .",
        "lint:css": "stylelint src/**/*.css",
        "posttest": "npm audit || true",
        "size": "source-map-explorer build/static/js/main.*"
    },
    "dependencies": {
        "babel-polyfill": "6.26.0",
        "d3-scale": "2.1.0",
        "d3-selection": "1.3.0",
        "d3-shape": "1.2.0",
        "dateformat": "3.0.3",
        "distinct-colors": "1.0.4",
        "dotenv": "6.0.0",
        "express": "4.16.3",
        "helmet": "3.13.0",
        "markdown-it": "8.4.2",
        "markdown-it-sup": "1.0.0",
        "memoize-one": "4.0.0",
        "metrics-graphics": "2.15.6",
        "morgan": "1.9.0",
        "npm-run-all": "4.1.3",
        "react": "16.4.2",
        "react-dom": "16.4.2",
        "react-ga": "2.5.3",
        "react-helmet": "5.2.0",
        "react-loadable": "5.4.0",
        "react-metrics-graphics": "1.0.6",
        "react-refetch": "1.0.4",
        "react-router-dom": "4.3.1",
        "react-scripts": "1.1.4",
        "react-spinners": "0.3.3",
        "whatwg-fetch": "2.0.4"
    },
    "devDependencies": {
        "babel-eslint": "8.2.6",
        "browserstack-local": "1.3.3",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-16": "1.1.1",
        "eslint": "5.3.0",
        "eslint-plugin-jest": "21.18.0",
        "eslint-plugin-json": "1.2.1",
        "eslint-plugin-jsx-a11y": "6.1.1",
        "eslint-plugin-react": "7.10.0",
        "geckodriver": "1.12.1",
        "jest-cli": "23.4.2",
        "nightwatch": "0.9.21",
        "react-test-renderer": "16.4.2",
        "request": "2.87.0",
        "selenium-server-standalone-jar": "3.13.0",
        "source-map-explorer": "1.5.0",
        "stylelint": "9.4.0",
        "stylelint-config-standard": "18.2.0"
    }
}
